{
  "exclude": [
    "**/test/**",
    "**/mocks/**",
    "**/examples/**",
    "**/samples/**",
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/.git/**",
    "**/__pycache__/**",
    "**/*.min.js",
    "**/*.bundle.js",
    "**/vendor/**",
    "**/migrations/*.py",
    "test_data/*.json"
  ],
  "exclude_patterns": [
    "RSA_KEY=.*",
    "password=test123",
    "example@example.com",
    "1234-5678-9012-3456",
    "ip_address=127.0.0.1",
    "ip_address=0.0.0.0",
    "api_key=fake",
    "example_user"
  ],
  "severity_threshold": "medium",
  "scan_settings": {
    "max_file_size": 1048576,
    "consider_non_code": false,
    "enhanced_patterns": true,
    "false_positive_threshold": 0.7
  },
  "documentation": {
    "markdown": false,
    "txt": false,
    "rst": false
  }
} 