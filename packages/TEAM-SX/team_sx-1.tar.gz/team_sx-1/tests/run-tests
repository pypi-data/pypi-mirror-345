#!/usr/bin/env python



""" Main front-end to the tests of SajadQ.

Has many options, read --help output.
"""

import os
import sys

# Unchanged, running from checkout, use the parent directory, the SajadMr
# package ought be there.
sys.path.insert(
    0,
    os.path.normpath(
        os.path.join(
            os.path.dirname(__file__),
            "..",
        )
    ),
)

# isort:start

from SajadMr.tools.testing.run_SajadMr_tests.__main__ import main

main()


