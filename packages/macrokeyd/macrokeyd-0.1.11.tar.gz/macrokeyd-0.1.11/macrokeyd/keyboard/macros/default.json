{
    "meta": {
        "author": "macrokeyd",
        "created_at": "2025-04-29",
        "version": "1.0",
        "target_device_name": "TEC-FX556K",
        "description": "Macros b√°sicos para teclas QWERTY"
    },
    "macros": {
        "KEY_Q": {
        "action": "command",
        "value": "xdotool key ctrl+alt+t"
        },
        "KEY_W": {
        "action": "text",
        "value": "hello world"
        },
        "KEY_E": {
        "action": "command",
        "value": "gnome-control-center"
        },
        "KEY_R": {
        "action": "command",
        "value": "xdg-open https://www.google.com"
        },
        "KEY_T": {
        "action": "command",
        "value": "gnome-terminal"
        },
        "KEY_Y": {
        "action": "command",
        "value": "python3 -c \"with open('/tmp/test-run-python.txt', 'w') as f: f.write('Hello world from your pad.')\""
        }
    }
}