    {
        "qualifiedName": "@zereight/mcp-gitlab",
        "displayName": "@zereight/mcp-gitlab",
        "remote": false,
        "githubUrl": "https://github.com/zereight/mcp-gitlab",
        "description": "用于GitLab操作的MCP服务器，支持仓库管理和代码操作",
        "icon": "DEFAULT_MCP_ICON",
        "connections": [
            {
                "type": "stdio",
                "stdioFunction": [
                    "npx"
                ],
                "published": true,
                "configSchema": {
                    "args": [
                        "-y",
                        "@zereight/mcp-gitlab"
                    ],
                    "env": {
                        "GITLAB_PERSONAL_ACCESS_TOKEN": {
                            "type": "string",
                            "description": "GITLAB_PERSONAL_ACCESS_TOKEN",
                            "required": true
                        },
                        "GITLAB_API_URL": {
                            "type": "string",
                            "description": "GITLAB_API_URL",
                            "default": "GITLAB_API_URL",
                            "required": true
                        },
                        "GITLAB_READ_ONLY_MODE": {
                            "type": "string",
                            "description": "GITLAB_READ_ONLY_MODE"
                        }
                    }
                }
            },
            {
                "type": "ws",
                "deploymentUrl": "wss://example.com",
                "published": true
            }
        ]
    },
    {
        "qualifiedName": "mcp-twikit",
        "displayName": "mcp-twikit (Mock)",
        "remote": false,
        "githubUrl": "https://github.com/adhikasp/mcp-twikit",
        "description": "Twitter集成服务器，支持发推文和管理Twitter账户",
        "icon": "DEFAULT_MCP_ICON",
        "connections": [
            {
                "type": "stdio",
                "stdioFunction": [
                    "uvx"
                ],
                "published": true,
                "configSchema": {
                    "args": [
                        "--from",
                        "git+https://github.com/adhikasp/mcp-twikit",
                        "mcp-twikit",
                        {
                            "name": "custom_arg",
                            "type": "string",
                            "description": "用户自定义参数",
                            "required": true
                        }
                    ],
                    "env": {
                        "TWITTER_USERNAME": {
                            "type": "string",
                            "description": "TWITTER_USERNAME",
                            "default": "@example",
                            "required": true
                        },
                        "TWITTER_EMAIL": {
                            "type": "string",
                            "description": "TWITTER_EMAIL",
                            "default": "me@example.com",
                            "required": true
                        },
                        "TWITTER_PASSWORD": {
                            "type": "string",
                            "description": "TWITTER_PASSWORD",
                            "default": "secret",
                            "required": true
                        }
                    }
                }
            }
        ]
    },
