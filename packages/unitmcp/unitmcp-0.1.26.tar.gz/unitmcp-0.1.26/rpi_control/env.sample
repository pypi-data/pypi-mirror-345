# Example .env file for rpi_control usage
# Used by both Python and shell scripts

# Raspberry Pi connection
RPI_HOST=raspberrypi
RPI_USERNAME=pi
RPI_PORT=8081

# Used by install_remote.sh
REMOTE=pi@raspberrypi
REMOTE_PATH=~/pi

# Used by start.sh/client.sh
SCRIPT_DIR=./rpi_control

# Default audio test files
DEFAULT_MP3=test.mp3
DEFAULT_WAV=test.wav
